{
  "name": "@mangrovedao/mangrove.js",
  "version": "0.11.4",
  "author": "Mangrove DAO",
  "description": "A Typescript SDK for the Mangrove Protocol.",
  "license": "(BSD-2-Clause OR BSD-3-Clause)",
  "main": "dist/nodejs/index.js",
  "browser": "dist/browser/mangrove.min.js",
  "scripts": {
    "package-version": "echo $npm_package_version",
    "precommit": "lint-staged",
    "prepack": "yarn run build",
    "lint": "npx eslint ./src/*.ts",
    "build-this-package": "npm-run-all -pn --aggregate-output typechain lint && npm-run-all -pn --aggregate-output tscbuild esbuild && yarn make-cli-executable",
    "heroku-build-this-package": "yarn typechain && yarn lint && yarn tscbuild && yarn esbuild",
    "build": "yarn install && yarn workspaces foreach -vpiR --topological-dev --from $npm_package_name run build-this-package",
    "make-cli-executable": "shx chmod u+x ./dist/nodejs/cli/mgv.js",
    "clean-this-package": "rimraf dist",
    "clean": "yarn workspaces foreach -vpiR --topological-dev --from $npm_package_name run clean-this-package",
    "test-with-dependencies": "yarn workspaces foreach -vpiR --topological-dev --from $npm_package_name run test",
    "test": "yarn run test:unit && yarn run test:integration",
    "test:integration": "cross-env POLYGON_NODE_URL=\"\" cross-env MUMBAI_NODE_URL=\"\" cross-env NODE_ENV=test nyc --reporter=lcov mocha --config test/mocha/config/integration-tests.json --exit",
    "test:unit": "cross-env NODE_ENV=test nyc --reporter=lcov mocha --config test/mocha/config/unit-tests.json --exit",
    "test:coverage": "cross-env POLYGON_NODE_URL=\"\" cross-env MUMBAI_NODE_URL=\"\" cross-env NODE_ENV=test nyc --reporter=lcov mocha --config test/mocha/config/coverage-tests.json --exit ",
    "typechain": "ts-node --transpileOnly src/util/runTypechain.ts",
    "doc": "cd src && yarn typedoc --options ../typedoc.json index.ts",
    "tscbuild": "tsc --build",
    "esbuild": "node esbuild.mjs"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md",
    "tsconfig.json"
  ],
  "imports": {
    "#package.json": "./package.json"
  },
  "bin": {
    "mgv": "./dist/nodejs/cli/mgv.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mangrovedao/mangrove.git"
  },
  "keywords": [
    "mangrove",
    "mangrove.js",
    "mangrove exchange",
    "decentralized finance",
    "defi",
    "ethereum"
  ],
  "nyc": {
    "include": [
      "src/**/*.ts"
    ]
  },
  "lint-staged": {
    "*.{js,ts,css,md,json,mjs}": "prettier --write"
  },
  "dependencies": {
    "@ethersproject/experimental": "^5.6.3",
    "@mangrovedao/commonlib.js": "workspace:*",
    "@mangrovedao/mangrove-core": "^1.1.3",
    "async-mutex": "^0.4.0",
    "big.js": "^6.1.1",
    "esbuild-plugin-resolve": "^1.0.3",
    "ethers": "^5.6.4",
    "node-cleanup": "^2.1.2"
  },
  "devDependencies": {
    "@espendk/json-file-reporter": "^1.4.2",
    "@ethersproject/abi": "^5.6.0",
    "@ethersproject/bytes": "^5.0.0",
    "@ethersproject/hardware-wallets": "^5.6.1",
    "@ethersproject/providers": "^5.6.4",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/big.js": "^6.1.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/shelljs": "^0.8.11",
    "@types/yargs": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dir-compare": "^4.0.0",
    "esbuild": "^0.15.13",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "mocha-multi-reporters": "^1.5.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "shx": "^0.3.4",
    "ts-essentials": "^9.1.2",
    "ts-mockito": "^2.0.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typedoc": "^0.23.18",
    "typescript": "^4.7.0",
    "yargs": "^17.3.0"
  }
}
